<tr>
  <td>{{ period.period }}</td>
  <td>{{ period.begin|date('d-m-Y') }}</td>
  <td>{{ period.end|date('d-m-Y') }}</td>
  <td class="actions">
    <ul>
      <li><a href="{{ path('GSimul_SAEditPeriod', {'id': period.id}) }}" title="Modifier la session '{{ period }}'">Modifier</a></li>
      <li><a href="{{ path('GSimul_SADeletePeriod', {'id': period.id}) }}" title="Supprimer la session '{{ period }}'" class="confirm">Supprimer</a></li>
    </ul>
  </td>
</tr>
