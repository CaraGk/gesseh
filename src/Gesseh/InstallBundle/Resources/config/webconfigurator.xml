<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="gesseh.install.webconfigurator.class">Gesseh\InstallBundle\Configurator\Configurator</parameter>
    </parameters>

    <services>
        <service id="gesseh.install.webconfigurator" class="%gesseh.install.webconfigurator.class%">
            <argument>%kernel.root_dir%</argument>
        </service>
    </services>
</container>

